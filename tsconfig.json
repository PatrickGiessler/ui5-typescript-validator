{
	"compilerOptions": {
		"module": "es2022",
		"moduleResolution": "node",
		"skipLibCheck": true,
		"allowJs": true,
		"strict": true,
		"strictNullChecks": false,
		"strictPropertyInitialization": false,
		"outDir": "./src-gen",
		"rootDir": "./",
		"paths": {
			"ui5/genericvalidator/*": ["./src/ui5/genericvalidator/*"]
		},
		"types": [
            "@sapui5/types",
			
        ]
	},
	"include": ["./src/**/*"]
}
